<?xml version="1.0" encoding="UTF-8"?>
<code_scheme name="acceptable-java-formatter">
    <!-- 들여쓰기 옵션 설정 -->
    <option name="OTHER_INDENT_OPTIONS">
        <value>
            <option name="INDENT_SIZE" value="2"/> <!-- 들여쓰기 크기 -->
            <option name="CONTINUATION_INDENT_SIZE" value="4"/> <!-- 계속되는 들여쓰기 크기 -->
            <option name="TAB_SIZE" value="2"/> <!-- 탭 크기 -->
            <option name="USE_TAB_CHARACTER" value="false"/> <!-- 탭 문자 사용 여부 -->
            <option name="SMART_TABS" value="false"/> <!-- 스마트 탭 사용 여부 -->
            <option name="LABEL_INDENT_SIZE" value="0"/> <!-- 레이블 들여쓰기 크기 -->
            <option name="LABEL_INDENT_ABSOLUTE" value="false"/> <!-- 레이블 들여쓰기 절대값 여부 -->
            <option name="USE_RELATIVE_INDENTS" value="false"/> <!-- 상대적 들여쓰기 사용 여부 -->
        </value>
    </option>

    <option name="INSERT_INNER_CLASS_IMPORTS" value="true"/> <!-- 내부 클래스의 import문 자동 추가 여부 -->
    <option name="CLASS_COUNT_TO_USE_IMPORT_ON_DEMAND" value="999"/> <!-- import문을 사용할 수 있는 클래스 수 -->
    <option name="NAMES_COUNT_TO_USE_IMPORT_ON_DEMAND" value="999"/> <!-- import문을 사용할 수 있는 이름 수 -->
    <option name="PACKAGES_TO_USE_IMPORT_ON_DEMAND">
        <value/>
    </option>

    <!-- import문 배치 설정 -->
    <option name="IMPORT_LAYOUT_TABLE">
        <value>
            <emptyLine/>
            <package name="" withSubpackages="true" static="true"/>
            <emptyLine/>
            <package name="java" withSubpackages="true" static="false"/>
            <emptyLine/>
            <package name="javax" withSubpackages="true" static="false"/>
            <emptyLine/>
            <package name="org" withSubpackages="true" static="false"/>
            <emptyLine/>
            <package name="net" withSubpackages="true" static="false"/>
            <emptyLine/>
            <package name="com" withSubpackages="true" static="false"/>
            <emptyLine/>
            <package name="" withSubpackages="true" static="false"/>
            <emptyLine/>
        </value>
    </option>

    <option name="RIGHT_MARGIN" value="120"/> <!-- 코드 오른쪽 여백 크기 -->

    <!-- Javadoc 설정 -->
    <option name="JD_ALIGN_PARAM_COMMENTS" value="false"/> <!-- Javadoc 파라미터 주석 정렬 여부 -->
    <option name="JD_ALIGN_EXCEPTION_COMMENTS" value="false"/> <!-- Javadoc 예외 주석 정렬 여부 -->
    <option name="JD_P_AT_EMPTY_LINES" value="false"/> <!-- Javadoc 빈 줄에 @param 태그 유지 여부 -->
    <option name="JD_KEEP_EMPTY_PARAMETER" value="false"/> <!-- Javadoc 빈 파라미터 주석 유지 여부 -->
    <option name="TERNARY_OPERATION_WRAP" value="1"/> <!-- 삼항 연산자 래핑 설정 -->
    <option name="TERNARY_OPERATION_SIGNS_ON_NEXT_LINE" value="true"/> <!-- 삼항 연산자 기호를 다음 줄로 이동할지 여부 -->
    <option name="FOR_STATEMENT_WRAP" value="1"/> <!-- for문 래핑 설정 -->
    <option name="ARRAY_INITIALIZER_WRAP" value="1"/> <!-- 배열 초기화자 래핑 설정 -->
    <option name="WRAP_COMMENTS" value="true"/> <!-- 주석 래핑 설정 -->
    <option name="IF_BRACE_FORCE" value="3"/> <!-- if문 괄호 강제 여부 -->
    <option name="DOWHILE_BRACE_FORCE" value="3"/> <!-- do-while문 괄호 강제 여부 -->
    <option name="WHILE_BRACE_FORCE" value="3"/> <!-- while문 괄호 강제 여부 -->
    <option name="FOR_BRACE_FORCE" value="3"/> <!-- for문 괄호 강제 여부 -->
    <option name="SPACE_BEFORE_ARRAY_INITIALIZER_LBRACE" value="true"/> <!-- 배열 초기화자 좌괄호 앞에 공백 추가 여부 -->

    <option name="BINARY_OPERATION_SIGN_ON_NEXT_LINE" value="true"/> <!-- 이항 연산자 기호를 다음 줄로 이동할지 여부 -->
    <option name="TERNARY_OPERATION_WRAP" value="1"/> <!-- 삼항 연산자 래핑 설정 -->
    <option name="TERNARY_OPERATION_SIGNS_ON_NEXT_LINE" value="true"/> <!-- 삼항 연산자 기호를 다음 줄로 이동할지 여부 -->
    <option name="FOR_STATEMENT_WRAP" value="1"/> <!-- for문 래핑 설정 -->
    <option name="ARRAY_INITIALIZER_WRAP" value="1"/> <!-- 배열 초기화자 래핑 설정 -->
    <option name="WRAP_COMMENTS" value="true"/> <!-- 주석 래핑 설정 -->
    <option name="IF_BRACE_FORCE" value="3"/> <!-- if문 괄호 강제 여부 -->
    <option name="DOWHILE_BRACE_FORCE" value="3"/> <!-- do-while문 괄호 강제 여부 -->
    <option name="WHILE_BRACE_FORCE" value="3"/> <!-- while문 괄호 강제 여부 -->
    <option name="FOR_BRACE_FORCE" value="3"/> <!-- for문 괄호 강제 여부 -->
    <option name="SPACE_BEFORE_ARRAY_INITIALIZER_LBRACE" value="true"/> <!-- 배열 초기화자 좌괄호 앞에 공백 추가 여부 -->

    <!-- Java 코드 스타일 설정 -->
    <codeStyleSettings language="JAVA">
        <option name="KEEP_CONTROL_STATEMENT_IN_ONE_LINE" value="false"/> <!-- 제어문을 한 줄에 유지할지 여부 -->
        <option name="KEEP_BLANK_LINES_IN_CODE" value="1"/> <!-- 코드에 빈 줄 유지 수 -->
        <option name="BLANK_LINES_AFTER_CLASS_HEADER" value="1"/> <!-- 클래스 헤더 다음에 빈 줄 유지 수 -->
        <option name="ALIGN_MULTILINE_PARAMETERS" value="false"/> <!-- 다중 줄 파라미터 정렬 -->
        <option name="ALIGN_MULTILINE_RESOURCES" value="false"/> <!-- 다중 줄 리소스 정렬 -->
        <option name="ALIGN_MULTILINE_FOR" value="false"/> <!-- 다중 줄 for문 정렬 -->
        <option name="CALL_PARAMETERS_WRAP" value="1"/> <!-- 메소드 호출 파라미터 래핑 설정 -->
        <option name="METHOD_PARAMETERS_WRAP" value="1"/> <!-- 메소드 파라미터 래핑 설정 -->
        <option name="EXTENDS_LIST_WRAP" value="1"/> <!-- 상속 목록 래핑 설정 -->
        <option name="THROWS_KEYWORD_WRAP" value="1"/> <!-- throws 목록 래핑 설정 -->
        <option name="METHOD_CALL_CHAIN_WRAP" value="1"/> <!-- 메소드 호출 체인 래핑 설정 -->
        <option name="BINARY_OPERATION_WRAP" value="1"/> <!-- 이항 연산자 래핑 설정 -->
        <option name="BINARY_OPERATION_SIGN_ON_NEXT_LINE" value="true"/> <!-- 이항 연산자 기호를 다음 줄로 이동할지 여부 -->
        <option name="TERNARY_OPERATION_WRAP" value="1"/> <!-- 삼항 연산자 래핑 설정 -->
        <option name="TERNARY_OPERATION_SIGNS_ON_NEXT_LINE" value="true"/> <!-- 삼항 연산자 기호를 다음 줄로 이동할지 여부 -->
        <option name="FOR_STATEMENT_WRAP" value="1"/> <!-- for문 래핑 설정 -->
        <option name="ARRAY_INITIALIZER_WRAP" value="1"/> <!-- 배열 초기화자 래핑 설정 -->
        <option name="WRAP_COMMENTS" value="true"/> <!-- 주석 래핑 설정 -->
        <option name="IF_BRACE_FORCE" value="3"/> <!-- if문 괄호 강제 여부 -->
        <option name="DOWHILE_BRACE_FORCE" value="3"/> <!-- do-while문 괄호 강제 여부 -->
        <option name="WHILE_BRACE_FORCE" value="3"/> <!-- while문 괄호 강제 여부 -->
        <option name="FOR_BRACE_FORCE" value="3"/> <!-- for문 괄호 강제 여부 -->
        <option name="PARENT_SETTINGS_INSTALLED" value="true"/>
        <indentOptions>
            <option name="INDENT_SIZE" value="2"/> <!-- 들여쓰기 크기 -->
            <option name="CONTINUATION_INDENT_SIZE" value="4"/> <!-- 계속되는 들여쓰기 크기 -->
            <option name="TAB_SIZE" value="2"/> <!-- 탭 크기 -->
            <option name="USE_TAB_CHARACTER" value="false"/> <!-- 탭 문자 사용 여부 -->
            <option name="SMART_TABS" value="false"/> <!-- 스마트 탭 사용 여부 -->
            <option name="LABEL_INDENT_SIZE" value="0"/> <!-- 레이블 들여쓰기 크기 -->
            <option name="LABEL_INDENT_ABSOLUTE" value="false"/> <!-- 레이블 들여쓰기 절대값 여부 -->
            <option name="USE_RELATIVE_INDENTS" value="false"/> <!-- 상대적 들여쓰기 사용 여부 -->
        </indentOptions>
    </codeStyleSettings>
</code_scheme>